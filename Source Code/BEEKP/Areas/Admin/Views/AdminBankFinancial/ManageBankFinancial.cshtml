

@model BEEKP.Areas.Admin.Models.BankFinancialManageModel
@{
    ViewBag.Title = "ManageBankFinancial";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

@using (Html.BeginForm("ManageBankFinancial", "AdminBankFinancial", FormMethod.Post, new { @id = "frmBankFinancial", @name = "frmBankFinancial" }))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(model => Model.BankFinancial.bank_financial_id)


    <div class="page-head">
        <div class="container">
            <!-- BEGIN PAGE TITLE -->
            <div class="page-title">
                <h1>Manage Bank Financial <small>statistics & reports</small></h1>
            </div>
            <!-- END PAGE TITLE -->

        </div>
    </div>
    <div class="page-content">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <!-- BEGIN VALIDATION STATES-->
                    <div class="portlet light">
                        <div class="portlet-title">
                            <div class="caption">
                                <span class="caption-subject font-green-sharp bold uppercase">Manage bank Financial</span> <small class="small-font-cuatom">Fields marked with asterisk (<span class="required-asterisk"> * </span>) are mandatory to fill in </small>
                            </div>
                        </div>
                        <div class="portlet-body form">
                            <div class="horizontal-form ">
                                <div class="form-body form-body-small-padding">
                                    @*<h3 class="form-section page-title mandatory-note">&nbsp <small>Fields marked with asterisk (<span class="required-asterisk"> * </span>) are mandatory to fill in </small></h3>*@

                                    <!--body start-->
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group clearfix">
                                                <label class="control-label col-md-4">Type Of Institution<span class="required">* </span></label>
                                                <div class="col-md-8">
                                                    @Html.DropDownListFor(model => Model.BankFinancial.institution_type_id, new SelectList(Model.ListTypeOfInstitution, "institution_type_id", "institution_type_name", ""), new { @class = "form-control required", @id = "ddlinstitution_type_name", autocomplete = "off" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group clearfix">
                                                <label class="control-label col-md-4">Type Of Cluster<span class="required">* </span></label>
                                                <div class="col-md-8">
                                                    @Html.DropDownListFor(model => Model.BankFinancial.cluster_id, new SelectList(Model.ListCluster, "cluster_id", "cluster_name", ""), new { @class = "form-control required", @id = "ddlcluster", autocomplete = "off" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group clearfix">
                                                <label class="control-label col-md-4">Organisation Name<span class="required">* </span></label>
                                                <div class="col-md-8">
                                                    @Html.TextBoxFor(model => model.BankFinancial.organization_name, new { @class = "form-control required", @id = "organization_name", @Placeholder = "Organization Name" })
                                                    @Html.ValidationMessageFor(model => model.BankFinancial.organization_name, "", new { @class = "text-denger" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group clearfix">
                                                <label class="control-label col-md-4">Address<span class="required">* </span></label>
                                                <div class="col-md-8">
                                                    @Html.TextBoxFor(model => model.BankFinancial.address, new { @class = "form-control required", @id = "address", @Placeholder = "Address" })
                                                    @Html.ValidationMessageFor(model => model.BankFinancial.address, "", new { @class = "text-denger" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group clearfix">
                                                <label class="control-label col-md-4">Fax<span class="required">* </span></label>
                                                <div class="col-md-8">
                                                    @Html.TextBoxFor(model => model.BankFinancial.fax, new { @class = "form-control required", @id = "fax", @Placeholder = "Fax" })
                                                    @Html.ValidationMessageFor(model => model.BankFinancial.fax, "", new { @class = "text-denger" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group clearfix">
                                                <label class="control-label col-md-4">Email-Id<span class="required">* </span></label>
                                                <div class="col-md-8">
                                                    @Html.TextBoxFor(model => model.BankFinancial.email_id, new { @class = "form-control required", @id = "email_id", @Placeholder = "Email-Id" })
                                                    @Html.ValidationMessageFor(model => model.BankFinancial.address, "", new { @class = "text-denger" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group clearfix">
                                                <label class="control-label col-md-4">Approval Status<span class="required">* </span></label>
                                                <div class="col-md-8">
                                                    Pending
                                                    @Html.RadioButtonFor(model => model.BankFinancial.approval_status, BEEKP.Class.Constants.APPROVAL_STATUS_PENDING)

                                                    Approved
                                                    @Html.RadioButtonFor(model => model.BankFinancial.approval_status, BEEKP.Class.Constants.APPROVAL_STATUS_APPROVED)

                                                    Rejected
                                                    @Html.RadioButtonFor(model => model.BankFinancial.approval_status, BEEKP.Class.Constants.APPROVAL_STATUS_REJECTED)


                                                    @Html.ValidationMessageFor(model => model.BankFinancial.approval_status, "", new { @class = "text-denger" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group clearfix">
                                                <label class="control-label col-md-4">Active</label>
                                                <div class="col-md-8">
                                                    @Html.CheckBoxFor(model => model.BankFinancial.status, new { @class = "icheck", @id = "Status", @Placeholder = "Status" })
                                                    @Html.ValidationMessageFor(model => model.BankFinancial.status, "", new { @class = "text-denger" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group clearfix">
                                                <label class="control-label col-md-4">Telephone<span class="required">* </span></label>
                                                <div class="col-md-8">
                                                    @Html.TextBoxFor(model => model.BankFinancial.telephone, new { @class = "form-control required", @id = "telephone", @Placeholder = "Telephone" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">

                                        </div>
                                    </div>
                                </div>
                                <!--Footer-->
                                <div class="form-actions right">
                                    <div class="row">
                                        <div class="col-md-offset-3 col-md-9">
                                            <button type="button" class="btn default" onclick="location.href='@Url.Action("Index", "AdminBankFinancial", new {})'">Back</button>
                                            <button type="submit" class="btn blue blue-custom" name="btnAction"><i class="fa fa-check"></i>Save</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- END FORM-->
                        </div>
                        <!-- END VALIDATION STATES-->
                    </div>
                </div>
            </div>

        </div>
    </div>

    <!-- BEGIN VALIDATION STATES-->



}

