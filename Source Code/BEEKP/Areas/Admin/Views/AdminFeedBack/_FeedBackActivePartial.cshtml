@model BEEKP.Areas.Admin.Models.AdminFeedBackViewModel
@if (Model.ListFeedBackActive != null)
{

    <table id="tblFeedBackActive" class="table table-striped table-hover table-bordered action-column-custom">

        <thead>
            <tr>
                <th>
                     Name
                </th>
                <th>
                    Email
                </th>
                <th>
                    Contact no.
                </th>
               
                <th align="center" valign="middle">
                    Feedback
                </th>
            </tr>
        </thead>
        <tbody>

            @{


                for (Int32 i = 0; i < Model.ListFeedBackActive.Count; i++)
                {



                    <tr>
                        <td>
                            <label>@Model.ListFeedBackActive[i].name_user</label>
                        </td>
                        <td>
                            <label>@Model.ListFeedBackActive[i].email_id </label>
                        </td>
                        <td>
                            <label>@Model.ListFeedBackActive[i].contact_no </label>
                        </td>

                        <td align="center" valign="middle">
                            <label>@Model.ListFeedBackActive[i].feedback_message </label>
                        </td>
                    </tr>
                }

            }


        </tbody>
    </table>
                }
