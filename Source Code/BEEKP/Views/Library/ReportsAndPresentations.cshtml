<style>
.breadcrumb-area .breadcrumb-menu ul li.active {
    font-weight: 700;
    color: #9ff3c2;
}
button.accordion:after {
    content: '\002B';
    color: #777;
    font-weight: bold;
    float: right;
    margin-left: 5px;
}
button.accordion.active:after {
    content: "\2212";
}
.accordion {
    background-color: #1da0542b;
    color: #297b37;
    margin-bottom: 20px;
    cursor: pointer;
    padding: 18px;
    width: 100%;
    border: none;
    text-align: left;
    outline: none;
    font-size: 16px;
    transition: 0.4s;
    font-weight: 600;
}
.active, .accordion:hover {
    background-color: #1da054a1;
}
.panel {
    border: 1px solid #70c393;
    padding-bottom: 18px;
    display: none;
    background-color: white;
    overflow: hidden;
    margin-bottom: 20px;
}
 .title {
    margin-bottom: 0px;
    position: relative;
    text-align: center;
}

.report-container .report-thumb a {
    background: #4990e1;
    display: block;
    position: relative;
    padding: 4px 10px;
    color: #fff;
    border-radius: 0;
    font-size: .9rem;
    text-decoration: none;
    text-align: center;
    margin-top:20px;
}


</style>


<div class="container pt-10 min-height-400">
    <!-- BEGIN PAGE CONTENT INNER -->
    <div class="row">
        <div class="col-md-10 col-md-offset-1">
            <div class="portlet light">

                <div id="divmanufacturers" class="datatable-field-height">
                    <section class="breadcrumb-area">
                        <div class="container">
                            <div class="row">
                                <div class="col-xl-12">
                                    <div class="inner-content clearfix">
                                        <div class="title_breadcrumb float-left">
                                            <h1>Reports and Presentations</h1>
                                        </div>
                                        <div class="breadcrumb-menu float-right">
                                            <ul class="clearfix">
                                                <li><a href="/">Resources</a></li>
                                                <li class="active">Reports and Presentations</li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    @*<h4 class="text-theme-colored">Reports and Presentations</h4>*@
                    <hr />
                    <ul class="list-group list-padding">
                        @for (Int32 j = 0; j < Model.ListKnowledgeBank.Count; j++)
                        {
                            if (j % 2 == 0)
                            {
                                <li class="list-group-item odd-bg-color">
                                    @if (Model.ListKnowledgeBank[j].file_name != null && Model.ListKnowledgeBank[j].file_name != "")
                                    {
                                        <span class="badge badge-custom"><a href="~/documents/Knowledge_Bank/@Model.ListKnowledgeBank[j].file_name" target="_blank">Preview</a><i class="fa fa-file-../pdf-o"></i> </span>
                                    }
                                    @Model.ListKnowledgeBank[j].knowledge_bank_name
                                </li>
                            }
                            else
                            {
                                <li class="list-group-item ">
                                    @if (Model.ListKnowledgeBank[j].file_name != null && Model.ListKnowledgeBank[j].file_name != "")
                                    {
                                        <span class="badge badge-custom"><a href="~/documents/Knowledge_Bank/@Model.ListKnowledgeBank[j].file_name" target="_blank">Preview</a><i class="fa fa-file-../pdf-o"></i> </span>
                                    }
                                    @Model.ListKnowledgeBank[j].knowledge_bank_name
                                </li>
                            }
                        }
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- END PAGE CONTENT INNER -->

