@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .useful-links .link li {
        display: block !important;
        opacity: 1 !important;
    }

    .news-events li {
        opacity: 1 !important;
    }

    .newsletter-wrap .form_icon {
        top: 70% !important;
    }
</style>
