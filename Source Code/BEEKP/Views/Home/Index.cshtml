<script>
    function CallPrint(full_bd) {
        var prtContent = document.getElementById(full_bd);
        var WinPrint = window.open('', '', 'letf=100,top=100,width=600,height=600');
        WinPrint.document.write(prtContent.innerHTML);
        WinPrint.document.close();
        WinPrint.focus();
        WinPrint.print();
        //WinPrint.close()
    }
</script>
<script>


    $(document).ready(function () {
        getImpactIndicators();
        getUpdatesAnnouncements();
        getNewsMedia();
    });

    function getImpactIndicators() {
        $.ajax(
            {
                type: 'Get',
                url: '/Home/ImpactIndicators',
                success:
                    function (response) {
                        $("#lblEnergy").text(response.EnergySavingAchieved);
                        $("#lblMSME").text(response.MSMEUnitsReached);
                        $("#lblEnergyEfficciency").text(response.EnergyEfficciencyMeasure);
                        $("#lblInvestor").text(response.InvestorParticipatingMsmeUnits);
                    },
                error:
                    function (response) {
                        alert("Error: " + response);
                    }
            });
    }

    function getUpdatesAnnouncements() {
        $("#updatesAnnouncements").empty();
        $.ajax(
            {
                type: 'Get',
                url: '/Home/_updatesAnnouncements',
                success:
                    function (response) {
                        $("#updatesAnnouncements").append(response);
                    },
                error:
                    function (response) {
                        alert("Error: " + response);
                    }
            });
    }

    function getNewsMedia() {
        $("#newsMedia").empty();
        $.ajax(
            {
                type: 'Get',
                url: '/Home/_newsMedia',
                success:
                    function (response) {
                        $("#newsMedia").append(response);
                    },
                error:
                    function (response) {
                        alert("Error: " + response);
                    }
            });
    }

    function Subscriptionpost() {
        var email_val = $("#idEMAIL").val();
        $.ajax({

            url: 'Home/Subscription?email_val=' + email_val,
            type: 'Get',
            success: function (result) {
                console.log(result);
                if (result["MessageId"] > 0) {
                    $('#idEMAIL').val('');
                    alert(result["Message"]);
                }
                else {
                    alert(result["Message"]);
                }
            },
            error: function (result) {
                isSuccess = result;

            }

        }).done(function () {
        });
    }
</script>