@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("Index", "ClusterDetails", FormMethod.Post, new { @id = "frmClusterDetails", @name = "frmClusterDetails" }))
