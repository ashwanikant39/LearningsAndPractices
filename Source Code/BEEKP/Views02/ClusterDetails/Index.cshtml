                                            <h1>Cluster Profile</h1>
                                        </div>
                                        <div class="breadcrumb-menu float-right">
                                            <ul class="clearfix">
                                                <li><a href="/">Resources</a></li>
                                                <li class="active">Cluster Profile</li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    <div id="divmanufacturers">
                        @*<h4 class="text-theme-colored">Cluster Profile</h4>
                        <hr />*@
                        <div id="acdPhase" class="panel-group accordion">
                            @for (Int32 i = 0; i < Model.ListClusterDetailsModel.Count; i++)
                            {
                                string phaseid = "Phases" + Model.ListClusterDetailsModel[i].Phases.phases_id.ToString();
                                string acdidcluster = "acdClusterDetails" + Model.ListClusterDetailsModel[i].Phases.phases_id.ToString();

                                <div class="panel">
                                    <div class="panel-title"> <a data-parent="#acdPhase" data-toggle="collapse" href="#@phaseid" class="" aria-expanded="true"><span class="open-sub"><i class="fas fa-plus-square-o"></i></span> @Model.ListClusterDetailsModel[i].Phases.phases_title</a> </div>
                                    <div id="@phaseid" class="panel-collapse collapse" role="tablist" aria-expanded="true">
                                        <div class="panel-content">

                                            @*//================================================*@

                                            <div id="@acdidcluster" class="panel-group accordion">
                                                @for (Int32 j = 0; j < Model.ListClusterDetailsModel[i].ListCluster.Count; j++)
                                                {

                                                    string clusterid = "Cluster" + Model.ListClusterDetailsModel[i].ListCluster[j].cluster_id.ToString();

                                                    <div class="panel">
                                                        <div class="panel-title"> <a data-parent="#@acdidcluster" data-toggle="collapse" href="#@clusterid" class="" aria-expanded="true"><i class="fas fa-plus-square-o"></i> <span class="open-sub"></span> @Model.ListClusterDetailsModel[i].ListCluster[j].cluster_name</a> </div>
                                                        <div id="@clusterid" class="panel-collapse collapse" role="tablist" aria-expanded="true">
                                                            <div class="panel-content">
                                                                @Html.Raw(Model.ListClusterDetailsModel[i].ListCluster[j].cluster_profile)
                                                                <div class="row">
                                                                    @if (Model.ListClusterDetailsModel[i].ListCluster[j].cluster_doc_hindi != null && Model.ListClusterDetailsModel[i].ListCluster[j].cluster_doc_hindi != "")
                                                                    {
                                                                        <div class="col-md-4">
                                                                            <a href="/documents/Cluster/@Model.ListClusterDetailsModel[i].ListCluster[j].cluster_doc_hindi" target="_blank">
                                                                                <i class="fas fa-download"></i> @Model.ListClusterDetailsModel[i].ListCluster[j].cluster_doc_hindi
                                                                            </a>
                                                                        </div>
                                                                    }
                                                                    @if (Model.ListClusterDetailsModel[i].ListCluster[j].cluster_doc_english != null && Model.ListClusterDetailsModel[i].ListCluster[j].cluster_doc_english != "")
                                                                    {
                                                                        <div class="col-md-4">
                                                                            <a href="/documents/Cluster/@Model.ListClusterDetailsModel[i].ListCluster[j].cluster_doc_english" target="_blank">
                                                                                <i class="fas fa-download"></i> @Model.ListClusterDetailsModel[i].ListCluster[j].cluster_doc_english
                                                                            </a>
                                                                        </div>
                                                                    }
                                                                    @if (Model.ListClusterDetailsModel[i].ListCluster[j].cluster_doc_marathi != null && Model.ListClusterDetailsModel[i].ListCluster[j].cluster_doc_marathi != "")
                                                                    {
                                                                        <div class="col-md-4">
                                                                            <a href="/documents/Cluster/@Model.ListClusterDetailsModel[i].ListCluster[j].cluster_doc_marathi" target="_blank">
                                                                                <i class="fas fa-download"></i> @Model.ListClusterDetailsModel[i].ListCluster[j].cluster_doc_marathi
                                                                            </a>
                                                                        </div>
                                                                    }
                                                                </div>
                                                            </div>

                                                        </div>

                                                    </div>
                                                }



                                            </div>

                                            @*//================================================*@




                                        </div>

                                    </div>

                                </div>
                            }


