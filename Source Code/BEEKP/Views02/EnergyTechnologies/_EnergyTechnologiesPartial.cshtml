
@model BEEKP.Models.EnergyTechnologiesViewModel

@if (Model.ListEnergyTechnology != null)
{
    <table id="tblListEnergyTechnology" class="table table-striped table-hover table-bordered">

        <thead>
            <tr>
                <th width="30%">
                   EE Measures Category  
                </th>
                <th width="30%">
                  Suggested  EE Measures
                </th>
                <th width="40%">
                    Cluster
                </th>
                
            </tr>
        </thead>
        <tbody>

            @for (Int32 i = 0; i < Model.ListEnergyTechnology.Count; i++)
                {
                <tr>
                    <td>
                        <label>@Model.ListEnergyTechnology[i].category_measure_name</label>
                    </td>

                    <td>
                        <label>@Model.ListEnergyTechnology[i].EE_measure </label>
                    </td>
                    <td>
                        <label>@Model.ListEnergyTechnology[i].clusters </label>
                    </td>
                </tr>
            }
        </tbody>
    </table>
}
