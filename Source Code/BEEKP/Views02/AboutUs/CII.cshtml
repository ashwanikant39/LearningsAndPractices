        font-size: 30px;
        color: #417d55;
        margin: 0px;
        font-weight: 600;
    }

    .col-md-2 {
        -webkit-box-flex: 0;
        flex: 0 0 20%;
        max-width: 20%;
        margin-top: 20px;
    }

    .owl-item active {
        background-color: white;
    }

    .report-container .report-thumb a {
        background: #ffffff;
        display: block;
        font-weight: 700;
        position: relative;
        padding: 4px 10px;
        color: #242323;
        border-radius: 0;
        font-size: .9rem;
        text-decoration: none;
        text-align: left;
    }

    .tab {
        overflow: hidden;
        border: 1px solid #19a6e0;
        background-color: #edfaff;
        margin-top: 15px;
    }
        /* Style the buttons inside the tab */

        .tab button {
            background-color: inherit;
            float: left;
            border: none;
            outline: none;
            cursor: pointer;
            padding: 10px 40px;
            transition: 0.3s;
            font-size: 20px;
        }
            /* Change background color of buttons on hover */
            .tab button:hover {
                background-color: #78aee3;
                color: white;
            }
            /* Create an active/current tablink class */

            .tab button.active {
                background-color: #009cdc;
                color: white;
            }
    /* Style the tab content */

    .tabcontent {
        display: none;
        padding: 6px 12px;
        border: 1px solid #ccc;
        border-top: none;
        margin-bottom: 20px;
    }

    .tabcontent {
        animation: fadeEffect 3s;
    }
</style>
