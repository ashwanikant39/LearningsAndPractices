    .breadcrumb-area .breadcrumb-menu ul li.active {
        font-weight: 700;
        color: #9ff3c2;
    }

    button.accordion:after {
        content: '\002B';
        color: #777;
        font-weight: bold;
        float: right;
        margin-left: 5px;
    }

    button.accordion.active:after {
        content: "\2212";
    }

    .accordion {
        background-color: #edfaff;
        color: #009cdc;
        margin-bottom: 20px;
        cursor: pointer;
        padding: 18px;
        width: 100%;
        border: none;
        text-align: left;
        outline: none;
        font-size: 15px;
        transition: 0.4s;
    }

        .active, .accordion:hover {
            background-color: #009cdc6e;
        }

    .panel {
        padding: 0 18px;
