@model BEEKP.Models.BankFinancialViewModel
@if (Model.ListBankFinancial != null)
{
    <table id="tblBankFinancial" class="table table-striped table-hover table-bordered table-small-padding table-Valign-middle">

        <thead>
            <tr>
                <th>
                    Type of Institute	
                </th>
                <th>
                    Organization Name
                </th>
                <th>
                    Address
                </th>
                <th>
                    Cluster
                </th>
                <th class="width-70 text-align-center">
                    Details
                </th>
            </tr>
        </thead>
        <tbody>

            @for (Int32 i = 0; i < Model.ListBankFinancial.Count; i++)
            {
                <tr>
                    <td>
                        @Model.ListBankFinancial[i].institution_type_name
                    </td>
                    <td>
                        @Model.ListBankFinancial[i].organization_name
                    </td>
                    <td>
                        @Model.ListBankFinancial[i].address
                    </td>
                    <td>
                        @Model.ListBankFinancial[i].cluster_name 
                    </td>
                    <td valign="middle" align="center">
                        <a href="@Url.Action("Detail", "BankFinancial", new { sbank_financial_id =BEEKP.Class.Encryption.Encrypt(Model.ListBankFinancial[i].bank_financial_id.ToString(), true)})" title="" class="custom-edit-tooltip"><i class="fas fa-eye"></i></a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
}