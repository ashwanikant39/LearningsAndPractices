@model BEEKP.Models.MSMEViewModel

@if (Model.ListMSME != null)
{
    <table id="tblMSME" class="table table-striped table-hover table-bordered table-small-padding table-Valign-middle">

        <thead>
            <tr>
                
                <th>
                    Cluster 
                </th>
                <th>
                    Sector Name
                </th>
                <th>
                    Name of Unit
                </th>
                <th class="width-70 text-align-center">
                    Details
                </th>
            </tr>
        </thead>
        <tbody>

            @for (Int32 i = 0; i < Model.ListMSME.Count; i++)
            {


                <tr>
                    <td>
                        @Model.ListMSME[i].cluster_name
                    </td>
                    <td>
                        @Model.ListMSME[i].sector_name 
                    </td>
                    <td>
                        @Model.ListMSME[i].unit_name 
                    </td>
                    <td valign="middle" align="center">
                       <a href="@Url.Action("Detail", "MSME", new { smsme_id =BEEKP.Class.Encryption.Encrypt(Model.ListMSME[i].msme_id.ToString(), true)})" title="" class="custom-edit-tooltip"><i class="fa fa-eye"></i></a>
                    </td>
                </tr>
            }

        </tbody>
    </table>
}
